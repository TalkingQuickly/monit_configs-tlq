check process memcached with match memcached
   group memcache
   start program = "/usr/bin/sudo /usr/sbin/service memcached start"
   stop program = "/usr/bin/sudo /usr/sbin/service memcached stop"
   if failed host 127.0.0.1 port 11211 protocol memcache then restart
   if 3 restarts within 6 cycles then timeout
